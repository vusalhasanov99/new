@{
    string dashBoardUrl = Configuration["DashboardUrl"];
    string clickIntUrl = Configuration["ClickInt"];
    string siteName = Configuration["WebSite"];
    string fullSiteName = siteName + ".com";
    // Define a helper method to get the link based on the PostBackType
    string GetLinkWithVisitId(PostBackType postBackType, string originalLink, int visitId)
    {
        if (originalLink is not null)
        {
            switch (postBackType)
            {
                case PostBackType.Raketech:
                    return originalLink + visitId;
                case PostBackType.Affilika:
                    return originalLink + "?visit_id=" + visitId;
                // Add more cases for other PostBackTypes as needed
                default:
                    return originalLink; // Default behavior
            }
        }
        else
        {
            return string.Empty;
        }
    }

    string GetImageLink(string originalLink)
    {

        if (originalLink is not null)
        {
            return dashBoardUrl + originalLink;
        }
        else
        {
            return string.Empty;
        }
    }

    string Capitalize(string str)
    {
        str = str.ToUpper().Substring(0, 1) + str.Substring(1);
        return str;
    }

    string LinkToBase64(string originalLink)
    {
        byte[] urlBytes = System.Text.Encoding.UTF8.GetBytes(originalLink);
        string base64 = Convert.ToBase64String(urlBytes);
        return base64;
    }

    string ByteArrToBase64(byte[] arr)
    {
        if (arr is not null)
        {
            return Convert.ToBase64String(arr);
        }

        return string.Empty;
    }

    string GetImageMimeTypeFromBase64(string base64String)
    {
        if (string.IsNullOrEmpty(base64String))
        {
            return string.Empty;
        }

        string mimeType = "image/jpeg"; // Varsayılan değer

        string base64Header = base64String.Substring(0, 5).ToLower();

        switch (base64Header.ToUpper())
        {
            case "/9j/4":
                mimeType = "image/jpeg";
                break;
            case "iVBOR":
                mimeType = "image/png";
                break;
            case "PHN2Z":
            case "PD94B":
            case "PCFET":
                mimeType = "image/svg+xml";
                break;
            case "R0lGOD":
                mimeType = "image/gif";
                break;
            case "UklGR":
                mimeType = "image/webp";
                break;
            case "Qk0":
                mimeType = "image/bmp";
                break;
            case "AAABAA":
                mimeType = "image/x-icon";
                break;
        }

        return mimeType;
    }

    string GetImageMimeTypeFromURL(string url)
    {
        if (string.IsNullOrEmpty(url))
        {
            return string.Empty;
        }

        // Varsayılan değer, genellikle bir resim URL'si için en yaygın format olduğundan
        string mimeType = "image/jpeg";

        // Dosya uzantısına göre MIME tipini ayarlama


        if (url.Contains(".png"))
        {
            mimeType = "image/png";
            return mimeType;
        }
        else if (url.Contains(".svg"))
        {
            mimeType = "image/svg+xml";
            return mimeType;
        }
        else if (url.EndsWith(".jpg") || url.EndsWith(".jpeg"))
        {
            mimeType = "image/jpeg";
            return mimeType;
        }
        else if (url.Contains(".gif"))
        {
            mimeType = "image/gif";
            return mimeType;
        }
        else if (url.Contains(".bmp"))
        {
            mimeType = "image/bmp";
            return mimeType;
        }
        else if (url.Contains(".webp"))
        {
            mimeType = "image/webp";
            return mimeType;
        }


        return mimeType;
    }


}

